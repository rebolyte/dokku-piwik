{
    "document_root": "/app",
    "login": "#USERNAME",
    "password": "#PASSWORD",
    "email": "#EMAIL",
    "site_name": "#SITE_NAME",
    "site_url": "#SITE_URL",
    "base_domain": "#BASE_DOMAIN",
    "session_save_handler": "dbtable",
    "geo_provider": "geoip_pecl",
    "database": {
        "host": "#DB_HOST",
        "adapter": "Pdo\\Mysql",
        "username": "#DB_USERNAME",
        "password": "#DB_PASSWORD",
        "dbname": "#DB_NAME",
        "tables_prefix": "piwik_"
    },
    "branding": {
        "header_url": "https://cdnjs.cloudflare.com/ajax/libs/emojione/2.2.6/assets/png/261d-1f3fb.png"
    },
    "privacy": {
        "anonymize_ip": false,
        "honor_do_not_track": false
    },
    "plugins": [
        "CorePluginsAdmin",
        "CoreAdminHome",
        "CoreHome",
        "WebsiteMeasurable",
        "Diagnostics",
        "CoreVisualizations",
        "Proxy",
        "API",
        "Widgetize",
        "Transitions",
        "LanguagesManager",
        "Actions",
        "Dashboard",
        "MultiSites",
        "UserLanguage",
        "DevicesDetection",
        "Goals",
        "Events",
        "UserCountry",
        "VisitsSummary",
        "VisitFrequency",
        "VisitTime",
        "VisitorInterest",
        "Monolog",
        "Login",
        "UsersManager",
        "SitesManager",
        "Installation",
        "CoreUpdater",
        "CoreConsole",
        "ScheduledReports",
        "UserCountryMap",
        "Live",
        "PrivacyManager",
        "ImageGraph",
        "SegmentEditor",
        "Insights",
        "Morpheus",
        "BulkTracking",
        "Resolution",
        "Intl",
        "DBStats",
        "CustomDimensions",
        "PlatformsReport"
    ],
    "deactivate_plugins": [],
    "extras": {
        "General": {
            "enable_framed_pages": 1,
            "enable_framed_settings": 1
        },
        "Tracker": {
            "trust_visitors_cookies": 1,
            "use_third_party_id_cookie": 1
        }
    },
    "options": {
        "Plugin_XXX_Settings": "Copy From Your Options Of A Manually Setup Plugin"
    }
}
